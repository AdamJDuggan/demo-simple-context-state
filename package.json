{
  "homepage": "https://AdamJDuggan.github.io/super-hooks-demo",
  "name": "super-hooks-demo",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "font-awesome": "^4.7.0",
    "react": "^17.0.2",
